# Application
NODE_ENV=development
PORT=3004
API_PREFIX=api

# CORS
CORS_ORIGIN=*

# Database PostgreSQL
DB_HOST=localhost
DB_PORT=5432
DB_USERNAME=postgres
DB_PASSWORD=postgres
DB_DATABASE=immo360_equipement
DB_SYNCHRONIZE=false
DB_LOGGING=true
DB_SSL=false

# RabbitMQ
RABBITMQ_ENABLED=true
RABBITMQ_URL=amqp://guest:guest@localhost:5672
RABBITMQ_QUEUE=equipement_queue

# JWT (à implémenter)
JWT_SECRET=your-secret-key-change-this-in-production
JWT_EXPIRATION=1d

# Logs
LOG_LEVEL=debug
